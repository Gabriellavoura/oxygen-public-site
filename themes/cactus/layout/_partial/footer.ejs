<footer id="footer">
  <div class="footer-left">
    <%= __('footer.copyright') %> &hearts; <%= __('footer.copyright1') %> <a
      href="https://protofy.xyz"><%= config.author%></a>
    <%= new Date().getFullYear() %>
  </div>
  <div style="float:right">
    <% for (var i in theme.social_links) { %> 
      <a href="<%- theme.social_links[i] %>"><span class="fab fa-<%= i %>"></span></a>
    <% } %>
    <a href="mailto:oxygen@protofy.xyz"><span class="fas fa-envelope"></span></a>
  </div>
  <div class="footer-right">
    
    <nav>
      <ul>
        <% for (var i in theme.nav) { %>
        <li><a href="<%- url_for(theme.nav[i]) %>"><%= __('nav.'+i).replace("nav.", "") %></a></li>
        <% } %>
      </ul>
    </nav>
  </div>
</footer>